{
  "manifest_version": 2,
  "name": "Bookmarks opener",
  "version": "0.1",
  "description": "Opens Opera's Bookmarks Page",
  "browser_action": {
    "default_title": "Open Bookmarks"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": ["<all_urls>", "activeTab", "*://*/*" , "downloads", "downloads.shelf"],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "MacCtrl+Q"
      },
      "description": "Opens Bookmarks Page"
    }
  }
}